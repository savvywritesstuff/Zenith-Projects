{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1420",
    "frontendDist": "../dist"
  },
  "package": {
    "productName": "zenith-project-manager",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "identifier": "com.zenith.project.manager"
    },
    "windows": [
      {
        "title": "Zenith Project Manager",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}